function fDhcpEnable(arg) {
  var dhcp_ebl=document.getElementById(arg);
  var status=dhcp_ebl.checked;
  if(arg == "ApDhcpDisable")
  {
    if(status) 
    {
      document.getElementById("ApIpAddress").disabled=true;
      document.getElementById("ApNetmask").disabled=true;
      document.getElementById("ApGateway").disabled=true;
    }
    else
    {
      document.getElementById("ApIpAddress").disabled=false;
      // document.getElementById("dhcp_IPAdd").setAttribute('disabled',false);
      document.getElementById("ApNetmask").disabled=false;
      document.getElementById("ApGateway").disabled=false;
    }
  }
  else if(arg == "ApScanStatus")
  {
    if(status) 
    {
      document.getElementById("ApScanInterval").disabled=false;
    }
    else
    {
      document.getElementById("ApScanInterval").disabled=true;
    }
  }
}
function f24G5GEnable(arg)
{
  var varEnable=document.getElementById(arg);
  var chkStatus=varEnable.checked;
  if (arg == "Ap2.4GEnable")
  {
    if(chkStatus)
    {
      document.getElementById("ApClientNumber2.4G").disabled=false;
      document.getElementById("ApHTMode2.4G").disabled=false;
      document.getElementById("ApTransmitPower2.4G").disabled=false;
      document.getElementById("ApMode2.4G").disabled=false;
      document.getElementById("ApChannel2.4G").disabled=false;
      document.getElementById("ApFragmentation2.4G").disabled=false;
      document.getElementById("ApRTSCTS2.4G").disabled=false;
      document.getElementById("ApESSID2.4G").disabled=false;
      document.getElementById("ApHideSSID2.4G").disabled=false;
      document.getElementById("ApSecurityMode2.4G").disabled=false;
      document.getElementById("ApIndexCipher2.4G").disabled=false;
      document.getElementById("ApKey5G").disabled=false;

      fSecurityMode('ApSecurityMode2.4G');
    }
    else
    {
      document.getElementById("ApClientNumber2.4G").disabled=true;
      document.getElementById("ApHTMode2.4G").disabled=true;
      document.getElementById("ApTransmitPower2.4G").disabled=true;
      document.getElementById("ApMode2.4G").disabled=true;
      document.getElementById("ApChannel2.4G").disabled=true;
      document.getElementById("ApFragmentation2.4G").disabled=true;
      document.getElementById("ApRTSCTS2.4G").disabled=true;
      document.getElementById("ApESSID2.4G").disabled=true;
      document.getElementById("ApHideSSID2.4G").disabled=true;
      document.getElementById("ApSecurityMode2.4G").disabled=true;
      document.getElementById("ApIndexCipher2.4G").disabled=true;
      document.getElementById("ApKey2.4G").disabled=true;

      fSecurityMode('ApSecurityMode2.4G', true);
    }
  }
  else if (arg == "Ap5GEnable")
  {
    if(chkStatus)
    {
      document.getElementById("ApClientNumber5G").disabled=false;
      document.getElementById("ApHTMode5G").disabled=false;
      document.getElementById("ApTransmitPower5G").disabled=false;
      document.getElementById("ApMode5G").disabled=false;
      document.getElementById("ApChannel5G").disabled=false;
      document.getElementById("ApFragmentation5G").disabled=false;
      document.getElementById("ApRTSCTS5G").disabled=false;
      document.getElementById("ApESSID5G").disabled=false;
      document.getElementById("ApHideSSID5G").disabled=false;
      document.getElementById("ApSecurityMode5G").disabled=false;
      document.getElementById("ApIndexCipher5G").disabled=true;
      document.getElementById("ApKey2.4G").disabled=false;

      fSecurityMode('ApSecurityMode5G');
    }
    else
    {
      document.getElementById("ApClientNumber5G").disabled=true;
      document.getElementById("ApHTMode5G").disabled=true;
      document.getElementById("ApTransmitPower5G").disabled=true;
      document.getElementById("ApMode5G").disabled=true;
      document.getElementById("ApChannel5G").disabled=true;
      document.getElementById("ApFragmentation5G").disabled=true;
      document.getElementById("ApRTSCTS5G").disabled=true;
      document.getElementById("ApESSID5G").disabled=true;
      document.getElementById("ApHideSSID5G").disabled=true;
      document.getElementById("ApSecurityMode5G").disabled=true;
      document.getElementById("ApIndexCipher5G").disabled=true;
      document.getElementById("ApKey5G").disabled=true;

      fSecurityMode('ApSecurityMode5G', true);
    }
  }
}
function fSecurityMode(arg, disbl)
{
  var securityModeSe=document.getElementById(arg);
  var seValue=securityModeSe.value;
  if (arg == "ApSecurityMode2.4G")
  {
    //if ((seValue == "3"||seValue == "4") && (disbl!==true))
    if(1&&2)console.log('ok');
    if(1)
    {
      document.getElementById("PrimaryIP2.4G").disabled=false;
      document.getElementById("PrimaryIPPort2.4G").disabled=false;
      document.getElementById("PrimaryIPKey2.4G").disabled=false;
      document.getElementById("SecondIP2.4G").disabled=false;
      document.getElementById("SecondIPPort2.4G").disabled=false;
      document.getElementById("SecondIPKey2.4G").disabled=false;
    }
    else
    {
      document.getElementById("PrimaryIP2.4G").disabled=true;
      document.getElementById("PrimaryIPPort2.4G").disabled=true;
      document.getElementById("PrimaryIPKey2.4G").disabled=true;
      document.getElementById("SecondIP2.4G").disabled=true;
      document.getElementById("SecondIPPort2.4G").disabled=true;
      document.getElementById("SecondIPKey2.4G").disabled=true;
    }
  }
  else if (arg == "ApSecurityMode5G")
  {
    //if (!(seValue == "3"||seValue == "4") || (disbl==true))
    if(1)
    {

      document.getElementById("PrimaryIP5G").disabled=false;
      document.getElementById("PrimaryIPPort5G").disabled=false;
      document.getElementById("PrimaryIPKey5G").disabled=false;
      document.getElementById("SecondIP5G").disabled=false;
      document.getElementById("SecondIPPort5G").disabled=false;
      document.getElementById("SecondIPKey5G").disabled=false;
    }
    else
    {
      document.getElementById("PrimaryIP5G").disabled=true;
      document.getElementById("PrimaryIPPort5G").disabled=true;
      document.getElementById("PrimaryIPKey5G").disabled=true;
      document.getElementById("SecondIP5G").disabled=true;
      document.getElementById("SecondIPPort5G").disabled=true;
      document.getElementById("SecondIPKey5G").disabled=true;
    }
  }
}
